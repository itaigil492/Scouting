<!DOCTYPE html>
<html lang="en">

<head>
    <% include templates/head.ejs %>
</head>

<body>
    <!-- START PAGE CONTAINER -->
    <div class="page-container">

        <% include templates/sidebar.ejs %>

            <!-- PAGE CONTENT -->
            <div class="page-content">

                <% include templates/xnavigation.ejs %>


                    <!-- PAGE CONTENT WRAPPER -->
                    <div class="page-content-wrap">

                        <div class="row">
                            <div class="col-md-12">

                                <div class="panel panel-default">
                                    <div class="panel-body">
                                        <div class="panel panel-warning">
                                            <div class="panel-heading">
                                                <h3 class="panel-title">Playoff games</h3>
                                                <ul class="panel-controls">
                                                    <li><a href="#" class="panel-fullscreen"><span class="fa fa-expand"></span></a></li>
                                                    <li><a href="#" class="panel-collapse"><span class="fa fa-angle-down"></span></a></li>
                                                    <li><a href="#" class="panel-refresh"><span class="fa fa-refresh"></span></a></li>
                                                    <li><a href="#" class="panel-remove"><span class="fa fa-times"></span></a></li>
                                                </ul>
                                            </div>
                                            <div class="panel-body">
                                                <% Playoffs.sort(function(a, b) {return parseInt(b['gameId']) - parseInt(a['gameId'])}).forEach(function(game) { console.log(game); %>
                                                    <div class="row">
                                                        <div class="col-md-2 game-title">
                                                            Game #<%= game['gameId'] %>
                                                        </div>
                                                        <div class="col-md-10">
                                                            <% if (game['blueTeams']) {  Array.prototype.forEach.call(game['blueTeams'], blueTeam => { %>
                                                            <div class="col-md-2">
                                                                <a href="/game?gameId=<%= game['gameId'] + '&teamNumber=' + blueTeam + "&type=Playoffs" %>" class="tile tile-default games-team blue-team" id="<%=game['gameId'] + '_' + blueTeam%>">
                                                                    <%= blueTeam %>
                                                                    <div class="informer informer-danger dir-tr"></div>
                                                                </a>
                                                            </div>
                                                            <% }) }; %>

                                                            <% if (game['redTeams']) {  Array.prototype.forEach.call(game['redTeams'], redTeam => { %>
                                                            <div class="col-md-2">
                                                                <a href="/game?gameId=<%= game['gameId'] + '&teamNumber=' + redTeam + "&type=Playoffs" %>" class="tile tile-default games-team red-team" id="<%=game['gameId'] + '_' + redTeam%>">
                                                                    <%= redTeam %>
                                                                    <div class="informer informer-danger dir-tr"></div>
                                                                </a>
                                                            </div>
                                                            <% }) }; %>
                                                        </div>
                                                    </div>
                                                <% }); %>
                                            </div>
                                            <div class="panel-footer">
                                                <button class="btn btn-primary pull-right">Button</button>
                                            </div>
                                        </div>

                                        <div class="panel panel-primary">
                                            <div class="panel-heading">
                                                <h3 class="panel-title">Qualification games</h3>
                                                <ul class="panel-controls">
                                                    <li><a href="#" class="panel-fullscreen"><span class="fa fa-expand"></span></a></li>
                                                    <li><a href="#" class="panel-collapse"><span class="fa fa-angle-down"></span></a></li>
                                                    <li><a href="#" class="panel-refresh"><span class="fa fa-refresh"></span></a></li>
                                                    <li><a href="#" class="panel-remove"><span class="fa fa-times"></span></a></li>
                                                </ul>
                                            </div>
                                            <div class="panel-body">
                                                <% Qualification.sort(function(a, b) {return parseInt(b['gameId']) - parseInt(a['gameId'])}).forEach(function(game) { console.log(game); %>
                                                    <div class="row">
                                                        <div class="col-md-2 game-title">
                                                            Game #<%= game['gameId'] %>
                                                        </div>
                                                        <div class="col-md-10">
                                                            <% if (game['blueTeams']) {  Array.prototype.forEach.call(game['blueTeams'], blueTeam => { %>
                                                            <div class="col-md-2">
                                                                <a href="/game?gameId=<%= game['gameId'] + '&teamNumber=' + blueTeam + "&type=Qualification" %>" class="tile tile-default games-team blue-team" id="<%=game['gameId'] + '_' + blueTeam%>">
                                                                    <%= blueTeam %>
                                                                    <div class="informer informer-danger dir-tr"></div>
                                                                </a>
                                                            </div>
                                                            <% }) }; %>

                                                            <% if (game['redTeams']) {  Array.prototype.forEach.call(game['redTeams'], redTeam => { %>
                                                            <div class="col-md-2">
                                                                <a href="/game?gameId=<%= game['gameId'] + '&teamNumber=' + redTeam  + "&type=Qualification" %>" class="tile tile-default games-team red-team" id="<%=game['gameId'] + '_' + redTeam%>">
                                                                    <%= redTeam %>
                                                                    <div class="informer informer-danger dir-tr"></div>
                                                                </a>
                                                            </div>
                                                            <% }) }; %>
                                                        </div>
                                                    </div>
                                                <% }); %>
                                            </div>
                                            <div class="panel-footer">
                                                <button class="btn btn-primary pull-right">Button</button>
                                            </div>
                                        </div>

                                        <div class="panel panel-info">
                                            <div class="panel-heading">
                                                <h3 class="panel-title">Practice games</h3>
                                                <ul class="panel-controls">
                                                    <li><a href="#" class="panel-fullscreen"><span class="fa fa-expand"></span></a></li>
                                                    <li><a href="#" class="panel-collapse"><span class="fa fa-angle-down"></span></a></li>
                                                    <li><a href="#" class="panel-refresh"><span class="fa fa-refresh"></span></a></li>
                                                    <li><a href="#" class="panel-remove"><span class="fa fa-times"></span></a></li>
                                                </ul>
                                            </div>
                                            <div class="panel-body">
                                                <% Practice.sort(function(a, b) {return parseInt(b['gameId']) - parseInt(a['gameId'])}).forEach(function(game) { console.log(game); %>
                                                    <div class="row">
                                                        <div class="col-md-2 game-title">
                                                            Game #<%= game['gameId'] %>
                                                        </div>
                                                        <div class="col-md-10">
                                                            <% if (game['blueTeams']) {  Array.prototype.forEach.call(game['blueTeams'], blueTeam => { %>
                                                            <div class="col-md-2">
                                                                <a href="/game?gameId=<%= game['gameId'] + '&teamNumber=' + blueTeam + "&type=Practice"%>" class="tile tile-default games-team blue-team" id="<%=game['gameId'] + '_' + blueTeam%>">
                                                                    <%= blueTeam %>
                                                                    <div class="informer informer-danger dir-tr"></div>
                                                                </a>
                                                            </div>
                                                            <% }) }; %>

                                                            <% if (game['redTeams']) {  Array.prototype.forEach.call(game['redTeams'], redTeam => { %>
                                                            <div class="col-md-2">
                                                                <a href="/game?gameId=<%= game['gameId'] + '&teamNumber=' + redTeam + "&type=Practice"%>" class="tile tile-default games-team red-team" id="<%=game['gameId'] + '_' + redTeam%>">
                                                                    <%= redTeam %>
                                                                    <div class="informer informer-danger dir-tr"></div>
                                                                </a>
                                                            </div>
                                                            <% }) }; %>
                                                        </div>
                                                    </div>
                                                <% }); %>
                                            </div>
                                            <div class="panel-footer">
                                                <button class="btn btn-primary pull-right">Button</button>
                                            </div>
                                        </div>

                                    </div>
                                </div>

                            </div>
                        </div>

                    </div>
                    <!-- END PAGE CONTENT WRAPPER -->
            </div>
            <!-- END PAGE CONTENT -->
    </div>
    <!-- END PAGE CONTAINER -->

    <% include templates/logoutMessagebox.ejs %>

        <% include templates/footer.ejs%>

    <script src="js/scouting/games/tag.js"></script>
    <script>
        var playoffs = <%- JSON.stringify(Playoffs) %>;
        var practice = <%- JSON.stringify(Practice) %>;
        var importantTeams = (<%- JSON.stringify(importantTeams) %>);
        var reviewedTeams = (<%- JSON.stringify(reviewedTeams) %>);
        var unwantedTeams = (<%- JSON.stringify(unwantedTeams) %>);

        console.log("playoffs: " + JSON.stringify(playoffs));
        console.log("practice: " + JSON.stringify(practice));
        console.log("importantTeams: " + JSON.stringify(importantTeams));
        console.log("reviewedTeams: " + JSON.stringify(reviewedTeams));
        console.log("unwantedTeams: " + JSON.stringify(unwantedTeams));
        if(window.attachEvent){
            window.attachEvent("onload",load);
         } else{
            window.addEventListener("load",load,true);
         }

         function load(){
            if (importantTeams)
             tag(importantTeams, "important");
            if (reviewedTeams)
             tag(reviewedTeams, "done");
            if (unwantedTeams)
             tag(unwantedTeams, "unwanted");
         }
    </script>


</body>


</html>
